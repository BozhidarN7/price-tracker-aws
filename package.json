    "aws-cdk": "2.1016.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.37.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.902.0",
    "@aws-sdk/util-dynamodb": "^3.902.0",
