{
  "name": "price-tracker-aws",
  "type": "module",
  "version": "0.1.0",
  "bin": {
    "price-tracker-aws": "bin/price-tracker-aws.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "watch": "tsc -w",
    "test": "jest",
